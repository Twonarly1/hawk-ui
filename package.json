{
  "name": "twonarly-ui",
  "description": "Twonarly (Beau Hawkinson) design system and library",
  "version": "0.1.5",
  "license": "MIT",
  "main": "build/ui.js",
  "module": "build/ui.mjs",
  "types": "build/ui.d.ts",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Twonarly1/twonarly-ui"
  },
  "exports": {
    ".": {
      "types": "./build/ui.d.ts",
      "require": "./build/ui.js",
      "import": "./build/ui.mjs"
    },
    "./index.css": "./build/ui.css"
  },
  "scripts": {
    "dev": "bun storybook",
    "build": "tsup",
    "lint": "",
    "storybook": "storybook dev --quiet --no-open -p 6006",
    "build-storybook": "storybook build",
    "changeset": "changeset",
    "release": "bun run build && changeset publish"
  },
  "resolutions": {
    "@zag-js/store": "^0.21.0"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@ark-ui/react": "^1.0.0",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@commitlint/types": "^18.4.0",
    "@omnidev/knit": "^0.1.2",
    "@park-ui/tailwind-plugin": "^0.13.0",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/nextjs": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.2",
    "eslint-plugin-storybook": "^0.6.15",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "storybook": "^7.5.3",
    "storybook-dark-mode": "^3.0.1",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^7.2.0",
    "typescript": "^5",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1"
